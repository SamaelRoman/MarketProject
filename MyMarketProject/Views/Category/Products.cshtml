@using PagedList.Mvc
@using MyMarketProject.Models

@model PagedList.IPagedList<Product>
@{
    ViewBag.Title = "";
}
<link rel="stylesheet" href="~/Content/PagedList.css" />
@foreach (var P in Model)
{
    @Html.Partial("_ProductPartial", P)
}
<div class="col-md-12 col-sm-12 col-lg-12 col-xs-12 container">
    <div class="btn-group col-md-offset-3">
       @Html.PagedListPager(Model, Page => Url.Action("Get", "Category", new { CategoryId = ViewBag.CategoryId, BrandId = ViewBag.BrandId, Page = Page }));
    </div>  
</div>